clean_debug: 
	rm -f $(OBJ_DEBUG) $(OUT_DEBUG)
	rm -rf bin/Debug
	rm -rf $(OBJDIR_DEBUG)/tests/src/runge_kutta_methods
	rm -rf $(OBJDIR_DEBUG)/tests/src
	rm -rf $(OBJDIR_DEBUG)/tests/src/test_wave_equation_1d
	rm -rf $(OBJDIR_DEBUG)/tests/src/test_wave_equation_2d
	rm -rf $(OBJDIR_DEBUG)/tests/src/test_heat_conduction_equation_1d
	rm -rf $(OBJDIR_DEBUG)/tests/src/test_heat_conduction_equation_2d
	rm -rf $(OBJDIR_DEBUG)

before_release: 
	test -d bin/Release || mkdir -p bin/Release
	test -d $(OBJDIR_RELEASE)/tests/src/runge_kutta_methods || mkdir -p $(OBJDIR_RELEASE)/tests/src/runge_kutta_methods
	test -d $(OBJDIR_RELEASE)/tests/src || mkdir -p $(OBJDIR_RELEASE)/tests/src
	test -d $(OBJDIR_RELEASE)/tests/src/test_wave_equation_1d || mkdir -p $(OBJDIR_RELEASE)/tests/src/test_wave_equation_1d
	test -d $(OBJDIR_RELEASE)/tests/src/test_wave_equation_2d || mkdir -p $(OBJDIR_RELEASE)/tests/src/test_wave_equation_2d
	test -d $(OBJDIR_RELEASE)/tests/src/test_heat_conduction_equation_1d || mkdir -p $(OBJDIR_RELEASE)/tests/src/test_heat_conduction_equation_1d
	test -d $(OBJDIR_RELEASE)/tests/src/test_heat_conduction_equation_2d || mkdir -p $(OBJDIR_RELEASE)/tests/src/test_heat_conduction_equation_2d
	test -d $(OBJDIR_RELEASE) || mkdir -p $(OBJDIR_RELEASE)

after_release: 

release: before_release out_release after_release

out_release: before_release $(OBJ_RELEASE) $(DEP_RELEASE)
	$(LD) $(LIBDIR_RELEASE) -o $(OUT_RELEASE) $(OBJ_RELEASE)  $(LDFLAGS_RELEASE) $(LIB_RELEASE)


clean_release: 
	rm -f $(OBJ_RELEASE) $(OUT_RELEASE)
	rm -rf bin/Release
	rm -rf $(OBJDIR_RELEASE)/tests/src/runge_kutta_methods
	rm -rf $(OBJDIR_RELEASE)/tests/src
	rm -rf $(OBJDIR_RELEASE)/tests/src/test_wave_equation_1d
	rm -rf $(OBJDIR_RELEASE)/tests/src/test_wave_equation_2d
	rm -rf $(OBJDIR_RELEASE)/tests/src/test_heat_conduction_equation_1d
	rm -rf $(OBJDIR_RELEASE)/tests/src/test_heat_conduction_equation_2d
	rm -rf $(OBJDIR_RELEASE)

.PHONY: before_debug after_debug clean_debug before_release after_release clean_release

